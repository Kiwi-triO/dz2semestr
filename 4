#include <iostream>
#include <stack>
#include <queue>
#include <cmath>

using namespace std;

bool isPrime(int n) {
    if (n <= 1)
        return false;
    for (int i = 2; i <= sqrt(n); i++) {
        if (n % i == 0)
            return false;
    }
    return true;
}

bool isPerfectSquare(int n) {
    int squareRoot = sqrt(n);
    return squareRoot * squareRoot == n;
}

int main() {
    setlocale(LC_ALL, "ru");
    int n, num, count = 0;
    stack<int> primeStack;
    queue<int> squareQueue;
    cout << "Введите кол-во чисел ";
    cin >> n;
    cout << "Введите числа ";
    for (int i = 0; i < n; i++) {
        cin >> num;
        if (isPrime(num))
            primeStack.push(num);
        else if (isPerfectSquare(num))
            squareQueue.push(num);
        else
            count++;
    }
    cout << "Простые числа ";
    while (!primeStack.empty()) {
        cout << primeStack.top() << " ";
        primeStack.pop();
    }
    cout << endl << "Квадраты ";
    while (!squareQueue.empty()) {
        cout << squareQueue.front() << " ";
        squareQueue.pop();
    }

    cout << endl << "Ни простых, ни квадратов " << count << endl;

    return 0;
}
